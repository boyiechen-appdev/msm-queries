<h1>List of all directors</h1>

<table>
  <% @list_of_directors.each do |director| %>
    <tr>
      <td><%= director.id %> </td>
      <td>
        <img src="https://robohash.org/<%= director.image %>" >
      </td>
      <td><%= director.name %> </td>
      <td><%= director.dob %> </td>
      <td>
        <a href="/directors/<%= director.id %>">
          Show details
        </a>
      </td>
    </tr>
  <% end %>
</table>
